				</categories>
			</filter>
			<count>2</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>DRNTF_FakeTail</li>
				</thingDefs>
			</filter>
			<count>1</count>
		</li>
	</ingredients>
	<fixedIngredientFilter>
		<categories>
			<li>Medicine</li>
		</categories>
		<thingDefs>
			<li>DRNTF_FakeTail</li>
		</thingDefs>
	</fixedIngredientFilter>
	<appliedOnFixedBodyParts>
		<li>DRNTF_Tail</li>
	</appliedOnFixedBodyParts>
	<addsHediff>DRNTF_FakeTail</addsHediff>
</RecipeDef>

